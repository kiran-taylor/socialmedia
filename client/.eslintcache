ame)) {\n      setLiked(true);\n    } else setLiked(false);\n  }, [user, likes]);\n\n  const [likePost] = useMutation(LIKE_POST, {\n    variables: { postid: id },\n    onError(err) {\n      setError(err.graphQLErrors[0].extensions.exception.errors);\n    },\n  });\n\n  const likeButton = user ? (\n    liked ? (\n      <Button color=\"red\">\n        <Icon name=\"heart\" />\n      </Button>\n    ) : (\n      <Button color=\"red\" basic>\n        <Icon name=\"heart\" />\n      </Button>\n    )\n  ) : (\n    <Button as={Link} to={`/login`} color=\"red\" basic>\n      <Icon name=\"heart\" />\n    </Button>\n  );\n  return (\n    <Button as=\"div\" labelPosition=\"right\" size=\"mini\" onClick={likePost}>\n      {likeButton}\n      <Label basic color=\"red\" pointing=\"left\">\n        {likesCount}\n      </Label>\n    </Button>\n  );\n}\n\nconst LIKE_POST = gql`\n  mutation likePost($postid: ID!) {\n    likes(postid: $postid) {\n      id\n      body\n      username\n      likes {\n        id\n        username\n      }\n      likesCount\n    }\n  }\n`;\nexport default LikeButton;\n","/home/kiran/github/socialmedia/client/src/util/graphql.js",[],"/home/kiran/github/socialmedia/client/src/pages/SinglePost.js",[],"/home/kiran/github/socialmedia/client/src/component/DeleteButton.js",[],{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","replacedBy":"87"},{"ruleId":"84","replacedBy":"88"},{"ruleId":"86","replacedBy":"89"},{"ruleId":"84","replacedBy":"90"},{"ruleId":"86","replacedBy":"91"},{"ruleId":"84","replacedBy":"92"},{"ruleId":"86","replacedBy":"93"},{"ruleId":"94","severity":1,"message":"95","line":9,"column":10,"nodeType":"96","messageId":"97","endLine":9,"endColumn":11},"no-native-reassign",["98"],"no-negated-in-lhs",["99"],["98"],["99"],["98"],["99"],["98"],["99"],"no-unused-vars","'_' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]